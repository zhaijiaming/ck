@using CKWMS.App_Code
@using CKWMS.Models
@model defineinfoViewModel
@{ViewBag.Title="数据定义添加";}

<div class="breadcrumbs" id="breadcrumbs">
<script type="text/javascript">
try { ace.settings.check('breadcrumbs', 'fixed') } catch (e) { }
</script>
<ul class="breadcrumb">
<li>
<i class="icon-home home-icon"></i>
<a href="/Home">首页</a>
</li>
<li>
<a href="/defineinfo">数据定义</a>
</li>
<li class="active"> 新增</li>
</ul>
</div>

<div class="page-content">
@using (Html.BeginForm("Save","defineinfo",FormMethod.Post,new{@role ="form" }))
{
@Html.AntiForgeryToken()
<input type="hidden" name="id"/>
<div class="page-header">
<p>
<button type="submit" class="btn btn-default"><i class="icon-save"></i>保存</button>
<button type="button" class="btn btn-default" onclick="javascript:history.back(-1); "><i class="icon-hand-left"></i>返回</button>
</p>
</div>
<div class="container">
<div class="form-group">
@Html.LabelFor(m=>m.ObjName,new{ @class ="control-label col-sm-2"})
<div class="col-sm-10">
@Html.TextBoxFor(m=>m.ObjName,new{@class="form-control"})
</div>
</div>
<div class="form-group">
@Html.LabelFor(m=>m.TBValue,new{ @class ="control-label col-sm-2"})
<div class="col-sm-10">
@Html.TextBoxFor(m=>m.TBValue,new{@class="form-control"})
</div>
</div>
<div class="form-group">
@Html.LabelFor(m=>m.TBType,new{ @class ="control-label col-sm-2"})
<div class="col-sm-10">
@Html.TextBoxFor(m=>m.TBType,new{@class="form-control"})
</div>
</div>
<div class="form-group">
@Html.LabelFor(m=>m.Lan1,new{ @class ="control-label col-sm-2"})
<div class="col-sm-10">
@Html.TextBoxFor(m=>m.Lan1,new{@class="form-control"})
</div>
</div>
<div class="form-group">
@Html.LabelFor(m=>m.Lan2,new{ @class ="control-label col-sm-2"})
<div class="col-sm-10">
@Html.TextBoxFor(m=>m.Lan2,new{@class="form-control"})
</div>
</div>
<div class="form-group">
@Html.LabelFor(m=>m.Lan3,new{ @class ="control-label col-sm-2"})
<div class="col-sm-10">
@Html.TextBoxFor(m=>m.Lan3,new{@class="form-control"})
</div>
</div>
</div>
}
</div>

