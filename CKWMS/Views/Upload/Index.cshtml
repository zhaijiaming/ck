
@model System.Web.HttpContextBase
@{
    ViewBag.Title = "UploadView";
    Layout = "~/Views/Shared/EmptyView.cshtml";
}

<h2>UploadView</h2>
<br />
<br />
@*new { enctype = "multipart/form-data" }比不可少，否则上传文件不会成功 *@
@using (Html.BeginForm("UploadOK", "Upload", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <text>选择上传文件：</text><input name="file" type="file" id="file" />
    <br />
    <br />
    <input type="submit" name="Upload" value="Upload" />
}
