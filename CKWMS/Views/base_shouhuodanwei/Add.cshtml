@using CKWMS.App_Code;
@{ViewBag.Title = "收货单位添加";}
@Html.GetFileUpload("ob_base_shouhuodanwei_yingyezhizhaotp", "yyzz", "z", "cus", 2, 0,new Dictionary<string, string>() { { "yyzz", "ob_base_shouhuodanwei_yingyezhizhaotp" },{ "xktp", "ob_base_shouhuodanwei_jingyingxuketp" } })
<div class="breadcrumbs" id="breadcrumbs">
    <script type="text/javascript">
        try { ace.settings.check('breadcrumbs', 'fixed') } catch (e) { }
    </script>
    <ul class="breadcrumb">
        <li>
            <i class="icon-home home-icon"></i>
            <a href="/Home">首页</a>
        </li>
        <li>
            <a href="/base_shouhuodanwei">收货单位</a>
        </li>
        <li class="active"> 新增</li>
    </ul>
</div>

<div class="page-content">
    <form role="form" action="/base_shouhuodanwei/Save" method="post">
        <input type="hidden" name="ob_base_shouhuodanwei_id" />
        <input type="hidden" id="btnSelect" value="" />
        <div class="page-header">
            <p>
                <button type="submit" class="btn btn-default"><i class="icon-save"></i>保存</button>
                <button type="button" class="btn btn-default" onclick="javascript:history.back(-1); "><i class="icon-hand-left"></i>返回</button>
            </p>
        </div>
        <div class="container">
            <div class="form-group">
                <label for="name" class="col-sm-2">货主</label>
                <div class="col-sm-10">
                    @*<input type="text" class="form-control" name="ob_base_shouhuodanwei_huozhuid">*@
                    @Html.SelectList_Auto("ob_base_shouhuodanwei_huozhuid", "货主", "都有")
                </div>
            </div>
            <div class="form-group">
                <label for="name" class="col-sm-2">单位名称</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" name="ob_base_shouhuodanwei_mingcheng">
                </div>
            </div>
            <div class="form-group">
                <label for="name" class="col-sm-2">营业执照编号</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" name="ob_base_shouhuodanwei_yingyezhizhaobh">
                </div>
            </div>
            <div class="form-group">
                <label for="name" class="col-sm-2">营业执照有效期</label>
                <div class="col-sm-10">
                    <input type="date" class="form-control" value="@DateTime.Now.ToString("yyyy-MM-dd")" name="ob_base_shouhuodanwei_yingyezhizhaoyxq">
                </div>
            </div>
            <div class="form-group">
                <label for="name" class="col-sm-2">营业执照照片</label>
                <div class="col-sm-6">
                    <input type="text" class="form-control" name="ob_base_shouhuodanwei_yingyezhizhaotp" id="ob_base_shouhuodanwei_yingyezhizhaotp">
                </div>
                <div class="col-sm-2">
                    <button class="form-control" data-toggle="modal" data-target="#myModalUpload" onclick="btn1()">上传</button>
                </div>
                <div class="col-sm-2">
                    <span class="form-control" id="yyzz">
                        <a>浏览</a>
                    </span>
                </div>
            </div>
            <div class="form-group">
                @*<iframe src="/upload"></iframe>*@
            </div>
            <div class="form-group">
                <label for="name" class="col-sm-2">经营许可编号</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" name="ob_base_shouhuodanwei_jingyingxukebh">
                </div>
            </div>
            <div class="form-group">
                <label for="name" class="col-sm-2">经营许可有效期</label>
                <div class="col-sm-10">
                    <input type="date" class="form-control" value="@DateTime.Now.ToString("yyyy-MM-dd")" name="ob_base_shouhuodanwei_jingyingxukeyxq">
                </div>
            </div>
            <div class="form-group">
                <label for="name" class="col-sm-2">经营许可照片</label>
                <div class="col-sm-6">
                    <input type="text" class="form-control" name="ob_base_shouhuodanwei_jingyingxuketp" id="ob_base_shouhuodanwei_jingyingxuketp">
                </div>
                <div class="col-sm-2">
                    <button class="form-control" data-toggle="modal" data-target="#myModalUpload" onclick="btn2()">上传</button>
                </div>
                <div class="col-sm-2">
                    <span class="form-control" id="xktp">
                        <a>浏览</a>
                    </span>
                </div>
</div>
            <div class="form-group">
                <label for="name" class="col-sm-2">首营状态</label>
                <div class="col-sm-10">
                    @*<input type="text" class="form-control" name="ob_base_shouhuodanwei_shouying">*@
                    @Html.SelectList_Common("ob_base_shouhuodanwei_shouying", "首营状态")
                </div>
            </div>
            <div class="form-group">
                <label for="name" class="col-sm-2">录入日期</label>
                <div class="col-sm-10">
                    <input type="date" class="form-control" value="@DateTime.Now.ToString("yyyy-MM-dd")" name="ob_base_shouhuodanwei_makedate">
                </div>
            </div>
            <div class="form-group">
                <label for="name" class="col-sm-2">输入人</label>
                <div class="col-sm-10">
                    @Html.SelectList_Auto("ob_base_shouhuodanwei_makeman", "userinfo", "fullname")
                </div>
            </div>
        </div>
    </form>
</div>
