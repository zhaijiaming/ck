@using CKWMS.App_Code;
@using CKWMS.Models
@model base_shouhuodanweiViewModel
@{ViewBag.Title = "收货单位添加";}
@Html.GetFileUpload("yingyezhizhaotp", "yyzz", "z", "cus", 3, 0, new Dictionary<string, string>() { { "yyzz", "yingyezhizhaotp" }, { "xktp", "jingyingxuketp" }, { "batp", "beiantp" } })

<script type="text/javascript">
    try { ace.settings.check('breadcrumbs', 'fixed') } catch (e) { }
    $(document).ready(function () {
        //时间
        $("#date_yingyezhizhaoyxq").datepicker();
        $("#date_jingyingxukeyxq").datepicker();
        $("#date_beianyxq").datepicker();
        $("#date_beianpzrq").datepicker();
        $("#date_xukepzrq").datepicker();
        $("#date_makedate").datepicker();
        $("#pl_Quyu").draggable({ handle: '.tuodong' });
        //验证
        $("#huozhuid").attr('required', 'required');
        $("#Mingcheng").attr('required', 'required');
        $("#Mingcheng").blur(function () {
            $.post("/base_shouhuodanwei/checkcode",
                {
                    sh: $("#Mingcheng").val(),
                    hz: $("#huozhuid").val()
                }, function (data) {
                    if (data == -1)
                        alert("收货单位：" + $("#Mingcheng").val() + "，已经存在！");
                    if (data == -2)
                        alert("收货单位必须选择货主！");
                });
        });
        //经营范围
        //$("#pl_Fanwei").hide();
        $("#pl_Fanwei").draggable({ handle: '.tuodong' });

        var bj = 0;
        GetFanwei1();
        Getquyu();

        //显示全部
        $("#bt_fanwei_showall").click(function () {
            var bj = 1;
            var tempid = "";
            $("#qxdatatable tr input[type=checkbox]").each(function () {
                if (this.checked) {
                    tempid = tempid + $(this).parent().find(".xulieid").val() + ";";
                }
            });
            var fw = document.getElementsByClassName('xulieid');
            var i = $("#jingyingfanweidm").val();
            var ti = i.split(";");
            //ti.splice(ti.length - 1);
            var xiaolei = new Array();
            var x = 0;
            for (var k = 0; k < ti.length; k++, x = 0) {
                for (var l = 0; l < fw.length; l++) {
                    if (ti[k] == $(".xulieid").eq(l).val()) {
                        x = 1;
                    }
                }
                if (x != 1) {
                    xiaolei.push(ti[k]);
                }
            }
            for (k = 0; k < xiaolei.length; k++) {
                tempid = tempid + xiaolei[k] + ";";
            }
            tempid = tempid.substring(0, tempid.length - 1);
            GetFanwei(bj, tempid);
        });

        //显示大类
        $("#bt_fanwei_showdalei").click(function () {
            var bj = 1;
            var tempid = "";
            $("#qxdatatable tr input[type=checkbox]").each(function () {
                if (this.checked) {
                    tempid = tempid + $(this).parent().find(".xulieid").val() + ";";
                }
            });
            GetFanwei1(bj, tempid);
        });
        //关闭窗口
        $("#pl_close").click(function () {
            $("#pl_Fanwei").hide();
            var bj = 0;
            $("#guanlifenlei").val("全部");
            GetFanwei1(bj);
        });
        //添加范围
        $("#bt_fanwei_add").click(function () {
            var _jyfw = "";
            var _jyfw1 = "";
            var _jyfw2 = "";
            var _jyfw3 = "";
            var _jyfwjm = "";
            var t = document.getElementsByClassName('xulieid');
            $("#qxdatatable tr input[type=checkbox]").each(function () {
                if (this.checked) {
                    switch ($(this).parent().parent().parent().siblings().find(".glfl").html()) {
                        case "1": _jyfw1 = _jyfw1 + $(this).parent().parent().parent().siblings().find(".bh").html() + "," + $(this).parent().parent().parent().siblings().find(".mc").html() + ";";
                            break;
                        case "2": _jyfw2 = _jyfw2 + $(this).parent().parent().parent().siblings().find(".bh").html() + "," + $(this).parent().parent().parent().siblings().find(".mc").html() + ";";
                            break;
                        case "3": _jyfw3 = _jyfw3 + $(this).parent().parent().parent().siblings().find(".bh").html() + "," + $(this).parent().parent().parent().siblings().find(".mc").html() + ";";
                            break;
                    }
                    //_jyfw = _jyfw + $(this).parent().parent().parent().siblings().find(".glfl").html() + "," + $(this).parent().parent().parent().siblings().find(".bh").html() + "," + $(this).parent().parent().parent().siblings().find(".mc").html() + ";";
                    _jyfwjm = _jyfwjm + $(this).parent().find(".xulieid").val() + ";";
                }
            });
            _jyfw = "三类:" + _jyfw3 + "\n" + "二类:" + _jyfw2 + "\n" + "一类:" + _jyfw1;
            if (_jyfw3 == "")
                _jyfw = _jyfw.replace("三类:\n", "");
            if (_jyfw2 == "")
                _jyfw = _jyfw.replace("二类:\n", "");
            if (_jyfw1 == "")
                _jyfw = _jyfw.replace("一类:", "");

            var _addok = confirm("确定要修改经营范围吗？");
            if (_addok) {
                if (_jyfw.length >= 512) {
                    alert("所选数据超过范围，请重新选择。")
                }
                else {
                    $("#jingyingfanwei").val(_jyfw);
                    $("#jingyingfanweidm").val(_jyfwjm);
                    $("#pl_Fanwei").hide();
                    var bj = "0";
                }
                $("#guanlifenlei").val("全部");
                GetFanwei1(bj);
            }

        });
        //全选
        $("#bt_fanwei_all").click(function () {
            $(".xulieid").prop("checked", true);
        })
        //全部取消
        $("#bt_fanwei_clear").click(function () {
            $(".xulieid").prop("checked", false);
        })
        //选择管理分类
        $("#guanlifenlei").change(function () {
            var fl = document.getElementsByClassName('glfl');
            var lb = $("#guanlifenlei").find("option:selected").text();
            if (lb == "一类") {
                for (var i = 0; i < fl.length; i++) {
                    if ($(".glfl").eq(i).text() == "1") {
                        $(".bg").eq(i).show();
                    }
                    else { $(".bg").eq(i).hide(); }
                }
            }
            if (lb == "二类") {
                for (var i = 0; i < fl.length; i++) {
                    if ($(".glfl").eq(i).text() == "2") {
                        $(".bg").eq(i).show();
                    }
                    else { $(".bg").eq(i).hide(); }
                }
            }
            if (lb == "三类") {
                for (var i = 0; i < fl.length; i++) {
                    if ($(".glfl").eq(i).text() == "3") {
                        $(".bg").eq(i).show();
                    }
                    else { $(".bg").eq(i).hide(); }
                }
            }
            if (lb == "全部") {
                $(".bg").show();
            }
        });
        //区域
        $("#qy_close").click(function () {
            $("#pl_Quyu").hide();
            Getqy();
        });
        $("#bt_quyu_add").click(function () {
            var valqy = $('input:radio[name="radiobox"]:checked').parent().parent().parent().siblings().find(".qymingcheng").html();
            if (valqy == null) {
                alert("没有选择区域哦");
            }
            else {
                $("#quyu").val(valqy);
                $("#pl_Quyu").hide();
            }
        });
        //单击tr打钩，双击取值
        $("#rydatatable").on("dblclick", "tr", function () {
            $("#quyu").val($(this).find(".qymingcheng").html());
            $("#pl_Quyu").hide();
        });
        $("#rydatatable").on("click", "tr", function () {
            if ($(this).find(":radio").is(':checked')) {
                $(this).find(":radio").prop("checked", false);
            } else {
                $(this).find(":radio").prop("checked", true);
            }
        });
        $("#qxdatatable").on("click", "tr", function () {
            if ($(this).find(":checkbox").is(':checked')) {
                $(this).find(":checkbox").prop("checked", false);
            } else {
                $(this).find(":checkbox").prop("checked", true);
            }
        });
        //submit
        $("#save_top").bind("click", function () {
            $("#form").submit();
        });
        $("#save_bottom").bind("click", function () {
            $("#form").submit();
        });
        //照片的点击事件
        $(".yyzz_browse").click(function () {
            var img_url = $("#yyzz a").attr('href');
            if (img_url != null && img_url != "") {
                //console.log(img_url);
                window.open(img_url);
            }
        });
        $(".xktp_browse").click(function () {
            var img_url = $("#xktp a").attr('href');
            if (img_url != null && img_url != "") {
                //console.log(img_url);
                window.open(img_url);
            }
        });
        $(".batp_browse").click(function () {
            var img_url = $("#batp a").attr('href');
            if (img_url != null && img_url != "") {
                //console.log(img_url);
                window.open(img_url);
            }
        });
        //'是否合作'绑定值
        $("#HezuoSF").val("false");//默认赋值
        $("#HezuoSF").change(function () {
            if ($("#HezuoSF").is(":checked")) {
                $("#HezuoSF").val("true");//选中=true
            } else {
                $("#HezuoSF").val("false");//不选中=false
            }
        });
        //'是否审查'绑定值
        $("#ShenchaSF").val("false");//默认赋值
        $("#ShenchaSF").change(function () {
            if ($("#ShenchaSF").is(":checked")) {
                $("#ShenchaSF").val("true");//选中=true
            } else {
                $("#ShenchaSF").val("false");//不选中=false
            }
        });
    });
    //点击修改or增加
    function Bt_Fanweiadd() {
        document.getElementById("pl_Fanwei").removeAttribute("hidden");
        $("#pl_Fanwei").show();
    }
    function GetFanwei1(a, b) {
        var bj = a;
        var tempid = b;
        var filltable = "";
        $.post(
                "/base_qixiemulu/GetQixiemulu1",
                {

                },
                function (data) {
                    for (var i in data) {
                        filltable = filltable + '<tr class="bg"><td><div class="checkbox"><label><input type="checkbox" class ="xulieid" value="';
                        filltable = filltable + data[i].ID;
                        filltable = filltable + '"></label></div></td><td style="width:80px"><span class="bh">';
                        filltable = filltable + data[i].Bianhao;
                        filltable = filltable + '</span></td><td style="width:150px"><span class="mc">';
                        filltable = filltable + data[i].Mingcheng;
                        filltable = filltable + '</span></td><td style="width:800px">';
                        filltable = filltable + data[i].Miaoshu;
                        filltable = filltable + '</td><td style="width:100px"><span class="glfl">';
                        filltable = filltable + data[i].GuanliFL;
                        filltable = filltable + '</span></td></tr>';
                    }
                    $("#pl_Fanwei tbody").html(filltable);
                    GetData(bj, tempid)
                });
    }
    function GetFanwei(a, b) {
        var bj = a;
        var tempid = b;
        var filltable = "";
        $.post(
                "/base_qixiemulu/GetQixiemulu",
                {
                    //qxml: _qxmlid
                },
                function (data) {
                    for (var i in data) {
                        filltable = filltable + '<tr class="bg"><td><div class="checkbox"><label><input type="checkbox" class ="xulieid" value="';
                        filltable = filltable + data[i].ID;
                        filltable = filltable + '"></label></div></td><td style="width:80px"><span class="bh">';
                        filltable = filltable + data[i].Bianhao;
                        filltable = filltable + '</span></td><td style="width:150px"><span class="mc">';
                        filltable = filltable + data[i].Mingcheng;
                        filltable = filltable + '</span></td><td style="width:800px">';
                        filltable = filltable + data[i].Miaoshu;
                        filltable = filltable + '</td><td style="width:100px"><span class="glfl">';
                        filltable = filltable + data[i].GuanliFL;
                        filltable = filltable + '</span></td></tr>';
                    }
                    $("#pl_Fanwei tbody").html(filltable);
                    GetData(bj, tempid)
                });
    }

    function Bt_Quyuadd() {
        Getquyu();
        document.getElementById("pl_Quyu").removeAttribute("hidden");
        $("#pl_Quyu").show();
    }

    function Getquyu() {
        var filltable = "";
        $.post(
            "/base_quyu/GetQuyu",
            {

            },
            function (data) {
                for (var i in data) {
                    filltable = filltable + '<tr style="display:table;width:100%;table-layout:fixed"><td style="width:5%"><div class="radio" ><label><input name="radiobox" type="radio" class="radioid" value="';
                    filltable = filltable + data[i].ID;
                    filltable = filltable + '"></label></div></td><td style="width:15%">';
                    filltable = filltable + data[i].Bianhao;
                    filltable = filltable + '</td><td style="width:20%"><span class="qymingcheng">';
                    filltable = filltable + data[i].Mingcheng;
                    filltable = filltable + '</td><td>';
                    filltable = filltable + data[i].Miaoshu;
                    filltable = filltable + '</td></tr>';
                }
                $("#pl_Quyu tbody").html(filltable);
                Getqy();
            });
    }
    //打勾
    function GetData(a, tempid) {
        var fw = document.getElementsByClassName('xulieid');
        var bj = a;
        var i = tempid;
        if (bj == 0) {
            var i = $("#jingyingfanweidm").val();
        }
        if (i != null) {
            var t = i.split(";");
        } else { t = "" }
        for (var k = 0; k < t.length; k++) {
            for (var l = 0; l < fw.length; l++) {
                if (t[k] == $(".xulieid").eq(l).val())
                    $("#qxdatatable tr input[type=checkbox]").eq(l).attr("checked", true);
            }
        }
    }

    function Getqy() {
        var qymc = document.getElementsByClassName('qymingcheng');
        var tempqy = $("#quyu").val();
        for (var i = 0; i < qymc.length; i++) {
            if (tempqy == $(".qymingcheng").eq(i).html())
                $("#rydatatable tr input[type=radio]").eq(i).prop("checked", true);
        }
    }
</script>
<div class="breadcrumbs" id="breadcrumbs">
    <ul class="breadcrumb">
        <li>
            <i class="icon-home home-icon"></i>
            <a href="/Home">首页</a>
        </li>
        <li>
            <a href="/base_shouhuodanwei">收货单位</a>
        </li>
        <li class="active"> 新增</li>
    </ul>
</div>

<div class="page-content">
    <div class="page-header">
        <button type="button" class="btn btn-app btn-xs btn-primary radius-4" onclick="javascript:history.back(-1); "><i class="icon-reply icon-only"></i><b>返回</b></button>
        <button type="button" id="save_top" class="btn btn-app btn-xs btn-primary radius-4">
            <i class="icon-save"></i><b>保存</b>
            <span class="badge badge-transparent">
                <i class="light-red icon-asterisk"></i>
            </span>
        </button>
    </div>
    <div class="wrapper" style="background: url('/Imgs/bg-cloud.png') repeat;">
        <div class="container">
            <div class="row">
                <div class="col-sm-8 col-sm-offset-2">
                    <div class="widget-box radius-4">
                        <div class="widget-header">
                            <h4><b>收货单位信息</b></h4>
                        </div>
                        <div class="widget-body">
                            <div class="widget-main no-padding">
                                @using (Html.BeginForm("Save", "base_shouhuodanwei", FormMethod.Post, new { @role = "form", @class = "form-horizontal", @id = "form" }))
                                {
                                    @Html.AntiForgeryToken()
                                    <input type="hidden" name="ob_base_shouhuodanwei_id" />
                                    <input type="hidden" id="btnSelect" value="" />
                                    <input type="hidden" id="MakeMan" name="MakeMan" value=@Session["user_id"] />
                                    <fieldset>
                                        @*货主*@
                                        <div class="form-group">
                                            <label for="huozhuid" class="col-sm-3 control-label"><i class="red glyphicon glyphicon-asterisk"></i> <b>货主</b></label>
                                            <div class="col-sm-8">
                                                @Html.SelectList_Auto("huozhuid", "货主", "都有")
                                            </div>
                                        </div>
                                        @*单位名称*@
                                        <div class="form-group">
                                            <label for="Mingcheng" class="col-sm-3 control-label"><i class="red glyphicon glyphicon-asterisk"></i> <b>单位名称</b></label>
                                            <div class="col-sm-8">
                                                <input type="text" id="Mingcheng" name="Mingcheng" class="form-control" placeholder="请输入单位名称" />
                                            </div>
                                        </div>
                                        @*营业执照编号*@
                                        <div class="form-group">
                                            <label for="yingyezhizhaobh" class="col-sm-3 control-label"><b>营业执照编号</b></label>
                                            <div class="col-sm-8">
                                                <input type="text" id="yingyezhizhaobh" name="yingyezhizhaobh" class="form-control" placeholder="请输入营业执照编号" />
                                            </div>
                                        </div>
                                        @*营业执照有效期*@
                                        <div class="form-group">
                                            <label for="date_yingyezhizhaoyxq" class="col-sm-3 control-label"><b>营业执照有效期</b></label>
                                            <div class="col-sm-8">
                                                <div class="input-group">
                                                    <input id="date_yingyezhizhaoyxq" name="yingyezhizhaoyxq" placeholder="请填写营业执照有效期" class="form-control" value="@DateTime.Now.ToString("yyyy-MM-dd")" type="date">
                                                    <span class="input-group-addon">
                                                        <i class="icon-calendar bigger-110"></i>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        @*营业执照照片*@
                                        <div class="form-group">
                                            <label for="YingyezhizhaoTP" class="col-sm-3 control-label"><b>营业执照照片</b></label>
                                            <div class="col-sm-8">
                                                <div class="input-group">
                                                    <input id="yingyezhizhaotp" name="yingyezhizhaotp" readonly placeholder="请上传照片" class="form-control" type="text">
                                                    <span class="input-group-btn">
                                                        <button type="button" data-toggle="modal" data-target="#myModalUpload" onclick="btn1()" class="btn btn-primary btn-sm">
                                                            <b>上传</b>
                                                            <i class="icon-cloud-upload icon-on-right bigger-110"></i>
                                                        </button>
                                                        <span hidden id="yyzz"></span>
                                                        <button type="button" class="btn btn-primary btn-sm yyzz_browse">
                                                            <b>浏览</b>
                                                            <i class="icon-camera icon-on-right bigger-110"></i>
                                                        </button>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        @*经营许可编号*@
                                        <div class="form-group">
                                            <label for="jingyingxukebh" class="col-sm-3 control-label"><b>经营许可编号</b></label>
                                            <div class="col-sm-8">
                                                <input type="text" name="jingyingxukebh" class="form-control" placeholder="请填写经营许可编号" />
                                            </div>
                                        </div>
                                        @*经营许可有效期*@
                                        <div class="form-group">
                                            <label for="date_jingyingxukeyxq" class="col-sm-3 control-label"><b>经营许可有效期</b></label>
                                            <div class="col-sm-8">
                                                <div class="input-group">
                                                    <input id="date_jingyingxukeyxq" name="jingyingxukeyxq" placeholder="经营许可有效期" class="form-control" value="@DateTime.Now.ToString("yyyy-MM-dd")" type="date">
                                                    <span class="input-group-addon">
                                                        <i class="icon-calendar bigger-110"></i>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        @*经营许可照片*@
                                        <div class="form-group">
                                            <label for="ShengchanxukeTP" class="col-sm-3 control-label"><b>经营许可照片</b></label>
                                            <div class="col-sm-8">
                                                <div class="input-group">
                                                    <input id="jingyingxuketp" name="jingyingxuketp" readonly placeholder="请上传照片" class="form-control" type="text">
                                                    <span class="input-group-btn">
                                                        <button type="button" data-toggle="modal" data-target="#myModalUpload" onclick="btn2()" class="btn btn-primary btn-sm">
                                                            <b>上传</b>
                                                            <i class="icon-cloud-upload icon-on-right bigger-110"></i>
                                                        </button>
                                                        <span hidden id="xktp"></span>
                                                        <button type="button" class="btn btn-primary btn-sm xktp_browse">
                                                            <b>浏览</b>
                                                            <i class="icon-camera icon-on-right bigger-110"></i>
                                                        </button>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        @*经营许可发证机关*@
                                        <div class="form-group">
                                            <label for="xukefzjg" class="col-sm-3 control-label"><b>经营许可发证机关</b></label>
                                            <div class="col-sm-8">
                                                <input type="text" name="xukefzjg" class="form-control" placeholder="请填写经营许可发证机关" />
                                            </div>
                                        </div>
                                        @*经营许可批准日期*@
                                        <div class="form-group">
                                            <label for="date_xukepzrq" class="col-sm-3 control-label"><b>经营许可批准日期</b></label>
                                            <div class="col-sm-8">
                                                <div class="input-group">
                                                    <input id="date_xukepzrq" name="xukepzrq" placeholder="经营许可批准日期" class="form-control" value="@DateTime.Now.ToString("yyyy-MM-dd")" type="date">
                                                    <span class="input-group-addon">
                                                        <i class="icon-calendar bigger-110"></i>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        @*经营范围*@
                                        <div class="form-group">
                                            <label for="ob_base_gongyingshang_jingyingfanwei" class="col-sm-3 control-label"><b>经营范围</b></label>
                                            <div class="col-sm-8">
                                                <button type="button" class="btn btn-primary btn-sm" id="bt_Fanweiadd" onclick="Bt_Fanweiadd()"><b>增加</b><i class="icon-hand-up icon-on-right bigger-110"></i></button>
                                                <textarea type="text" style="resize:vertical;" rows="6" class="form-control" readonly id="jingyingfanwei" name="jingyingfanwei"></textarea>
                                            </div>
                                        </div>
                                        @*经营范围代码*@
                                        <div class="form-group" style="display:none">
                                            <label for="name" class="col-sm-2">经营范围代码</label>
                                            <div class="col-sm-10">
                                                <input type="text" readonly class="form-control" id="jingyingfanweidm" name="jingyingfanweidm">
                                            </div>
                                        </div>
                                        @*区域*@
                                        <div class="form-group">
                                            <label for="quyu" class="col-sm-3 control-label"><b>区域</b></label>
