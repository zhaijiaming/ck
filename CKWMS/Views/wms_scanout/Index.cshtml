@using CKWMS.App_Code
@using CKWMS.Models
@{
    ViewBag.Title = "扫描出库列表";
    Layout = "~/Views/Shared/WinCEView.cshtml";
}
<div>
    <h2><a href="/wms_scanhome">Home</a>/扫描出库</h2>
    <div class="row">
        <table class="table table-condensed table-hover" id="mydatatable">
            <caption></caption>
            <thead>
                <tr>
                    <td></td>
                    <td>出库编号</td>
                    <td>货主</td>
                    <td>客户</td>
                    <td>出库日期</td>
                    <td>状态</td>
                    <td></td>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in ViewBag.wms_chukudan)
                {
                    <tr>
                        <td>
                            <label><input type="checkbox" value="@item.ID"></label>
                        </td>
                        <td>@item.ChukudanBH</td>
                        <td>@Html.GetDataValue_ID("货主", "简称", (int)item.HuozhuID)</td>
                        <td>@item.KehuMC</td>
                        <td>@string.Format("{0:d}", item.ChukuRQ)</td>
                        <td>@string.Format("{0:s}",item.JihuaZT==2?"完成":"待拣")</td>
                        <td><span style="font-size:30px;"><a href="/wms_scanout/picking/?ckd=@item.ID">拣货</a></span></td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>

