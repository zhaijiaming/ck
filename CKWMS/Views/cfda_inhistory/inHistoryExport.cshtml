@using CKWMS.App_Code
@using CKWMS.Models
@using X.PagedList
@using X.PagedList.Mvc
@{
    ViewBag.Title = "历史入库清单";
    string _searchcondition = ViewBag.SearchCondition;
}

<div class="page-content">
    <div class="row">
        <table class="table table-condensed table-hover" id="mydatatable">
            <caption></caption>
            <thead>
                <tr>
                    <td>原序号</td>
                    <td>委托方企业名称</td>
                    <td>入库日期</td>
                    <td>入库类型</td>
                    <td>产品名称</td>
                    <td>规格(型号)</td>
                    <td>生产企业</td>
                    <td>生产企业许可证号/备案凭证号</td>
                    <td>生产批号/序列号</td>
                    <td>生产日期和有效期(或者失效期)</td>
                    <td>数量</td>
                    <td>单位</td>
                    <td>储运条件</td>
                    <td>贮存货位号</td>
                    <td>质量状态</td>
                    <td>备注</td>
                    <td>制单日期</td>
                    <td>制单人</td>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in ViewBag.cfda_inhistory)
                {
                    <tr>
                        <td>@item.XH</td>
                        <td>@item.WTFQYMC</td>
                        <td>@item.RKRQ</td>
                        <td>@item.RKLX</td>
                        <td>@item.CPMC</td>
                        <td>@item.GGXH</td>
                        <td>@item.SCQY</td>
                        <td>@item.SCXKZ</td>
                        <td>@item.SCPH</td>
                        <td>@item.YXQ</td>
                        <td>@item.SL</td>
                        <td>@item.DW</td>
                        <td>@item.CYTJ</td>
                        <td>@item.HW</td>
                        <td>@item.ZLZT</td>
                        <td>@item.BZ</td>
                        <td>@item.MakeDate</td>
                        <td>@item.MakeMan</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
