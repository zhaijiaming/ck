@{
    ViewBag.Title = "收货清单表";
    Layout = "~/Views/Shared/EmptyView.cshtml";
}
<script type="text/javascript">
    $(window).load(function () {
        var rkxdid = $("#rkxdid").val();
        document.getElementById("myReport").src = "../../reports/ReportZY.aspx?pid=RuKuXiangDan&rkxdid=" + rkxdid;
        $("#autoUpdate").show();
    });
</script>
<div id="autoUpdate" style="display:none; overflow-y: auto;background-color:white">
    <input type="hidden" id="rkxdid" value="@ViewBag.rkxdid" />
    <table style="height:100%;width:100%">
        <tr>

            <td></td>
        </tr>
        <tr><td class="center"><iframe id="myReport" style="width:1110px;height:800px;"></iframe></td></tr>
    </table>
</div>


