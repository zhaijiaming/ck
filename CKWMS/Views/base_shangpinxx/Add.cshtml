                    @Html.CheckBoxFor(m => m.ShenchaSF, new { @class = "form-control" })
                </div>
            </div>
            @*是否经营*@
            <div class="form-group">
                @Html.LabelFor(m => m.JingyinSF, new { @class = "control-label col-sm-2" })
                <div class="col-sm-10">
                    @Html.CheckBoxFor(m => m.JingyinSF, new { @class = "form-control" })
                </div>
            </div>
            @*商品条码*@
            <div class="form-group">
                <label for="name" class="col-sm-2">商品条码</label>
                <div class="col-sm-10">
                    @Html.TextBoxFor(m => m.ShangpinTM, new { @class = "form-control" })
                </div>
            </div>
            @*首营状态*@
            <div class="form-group">
                @Html.LabelFor(m => m.Shouying, new { @class = "control-label col-sm-2" })
                <div class="col-sm-10">
                    @*@Html.SelectList_Common("shouying", "首营状态")*@
                    <input type="text" class="form-control" readonly value="@Html.GetCommonValue_ID("首营状态", 1)">
                </div>
            </div>
            @*录入日期*@
            <div class="form-group">
                @Html.LabelFor(m => m.MakeDate, new { @class = "control-label col-sm-2" })
                <div class="col-sm-10">
                    <input type="text" readonly class="form-control" name="makedate" value="@DateTime.Now.ToString("yyyy-MM-dd")" />
                </div>
            </div>
            <div class="form-group">
                @Html.LabelFor(m => m.MakeMan, new { @class = "control-label col-sm-2" })
                <div class="col-sm-10">
                    <input type="text" class="form-control" readonly value="@Html.GetDataValue_ID("userinfo", "全名", (int)ViewBag.userid)" />
                </div>
            </div>
        </div>
    }
</div>
