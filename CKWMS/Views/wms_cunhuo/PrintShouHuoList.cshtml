@{
    ViewBag.Title = "上架报告单表";
    Layout = "~/Views/Shared/EmptyView.cshtml";
}
<script type="text/javascript">
    $(window).load(function () {
        var sjdid = $("#sjdid").val();
        document.getElementById("myReport").src = "../../reports/ReportZY.aspx?pid=ShangJiaList&sjdid=" + sjdid;
        $("#autoUpdate").show();
    });
</script>
<div id="autoUpdate" style="display:none; overflow-y: auto;background-color:white">
    <input type="hidden" id="sjdid" value="@ViewBag.sjdid" />
    <table style="height:100%;width:100%">
        <tr>

            <td></td>
        </tr>
        <tr><td class="center"><iframe id="myReport" style="width:1070px;height:800px;"></iframe></td></tr>
    </table>
</div>

