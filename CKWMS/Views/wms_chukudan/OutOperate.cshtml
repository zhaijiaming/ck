    </div>
</div>
<div id="pl_xlm" class="panel panel-primary" style="display:none;position:fixed;top:100px;left:200px;width:500px;height:500px;z-index:999;">
    <div class="panel-heading">
        <h3 class="panel-title">登记出库单的序列码<button id="pl_xlm_close" class="pull-right">X</button></h3>
    </div>
    <div class="panel-body">
        <div>
            <div class="col-sm-4"><p>总数：<label id="totalnum"></label></p></div>
            <div class="col-sm-2">
                <p class="btn default" id="pl_xlm_check"><i class="icon-check-minus"></i>复核</p>                
            </div>
            <div class="col-sm-2"><p class="btn default" id="pl_xlm_clear"><i class="icon-ok"></i>清除</p></div>
            <div class="col-sm-2"><p class="btn default" id="pl_xlm_ok"><i class="icon-ok"></i>确定</p></div>
            <div class="col-sm-2"><p class="btn default" id="pl_xlm_cancel"><i class="icon-search"></i>取消</p></div>
        </div>
        <div class="center">
            <textarea id="xlm" style="width:480px;height:400px;overflow:auto"></textarea>
        </div>
    </div>
</div>
<div id="pl_kd" class="panel panel-primary" style="display:none;position:fixed;top:100px;left:200px;width:400px;height:300px;z-index:999;">
    <div class="panel-heading">
        <h3 class="panel-title">快递登记<button id="pl_kd_close" class="pull-right">X</button></h3>
    </div>
    <div class="panel-body">
        <div class="center">
            <div class="col-sm-6"><p class="btn default" id="pl_kd_ok"><i class="icon-ok"></i>保存</p></div>
            <div class="col-sm-6"><p class="btn default" id="pl_kd_cancel"><i class="icon-search"></i>取消</p></div>
        </div>
        <br /><br />
        <div class="row">
            <div class="form-group">
                <label class="col-sm-3">运送方式</label>
                <div class="col-sm-9">
                    @Html.SelectList_Common("YunsongFS", "运送方式")
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-3">运输公司</label>
                <div class="col-sm-9">
                    @Html.SelectList_Auto("Kuaidi", "运输公司", "简称")
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-3">运单编号</label>
                <div class="col-sm-9">
                    <input type="text" id="ydhm" name="KDdanhao" class="form-control" />
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-3">结算方式</label>
                <div class="col-sm-9">
                    @Html.SelectList_Common("JiesuanFS", "结算方式")
                </div>
            </div>
        </div>
    </div>
</div>
