    <div class="row">
        <table class="table table-condensed table-hover" id="mydatatable">
            <caption></caption>
            <thead>
                <tr>
                    <td></td>
                    <td>出库编号</td>
                    <td>货主</td>
                    <td>客户名称</td>
                    <td>送货地址</td>
                    <td>出库日期</td>
                    <td>储运要求</td>
                    <td>客户单号</td>
                    <td>运单编号</td>
                    <td>状态</td>
                    <td>备注</td>
                    <td>录入</td>
                    <td>列表</td>
                    <td>选货</td>
                    <td>拣货</td>
                    <td>复核</td>
                    <td>序列</td>
                    <td>快递</td>
                    <td>详单</td>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in ViewBag.wms_chukudan)
                {
                    <tr>
                        <td>
                            <label>
                                <input type="checkbox" value="@item.ID"
                                       data-hz="@item.HuozhuID"
                                       data-ly="@item.XinxiLY"
                                       data-lx="@item.YewuLX"
                                       data-kh="@item.KehuID"
                                       data-fh="@item.FuheSF"
                                       data-jg="@item.JianguanSF"
                                       data-jh="@item.JihuaID"
                                       data-kd="@item.Kuaidi"
                                       data-ys="@item.YunsongFS"
                                       data-js="@item.JiesuanFS"
                                       data-dh="@item.KDdanhao"
                                       data-tel="@item.LianxiDH"
                                       data-man="@item.Lianxiren"
                                       data-addr="@item.Yunsongdizhi">
                            </label>
                        </td>
                        <td>@item.ChukudanBH</td>
                        <td class="huozhuid">@Html.GetDataValue_ID("货主", "简称", (int)item.HuozhuID)</td>
                        <td>@item.KehuMC</td>
                        <td>@item.Yunsongdizhi</td>
                        <td>@string.Format("{0:yyyy-MM-dd}", item.ChukuRQ == null ? "" : item.ChukuRQ)</td>
                        <td>@item.ChunyunYQ</td>
                        <td>@item.KehuDH</td>
                        <td>@item.KDdanhao</td>
                        <td>@Html.GetCommonValue_ID("出库状态", (int)item.JihuaZT)</td>
                        <td title="@item.Beizhu">@string.Format("{0:s}", item.Beizhu == null ? "" : item.Beizhu.Length > 12 ? item.Beizhu.Substring(0, 12) : item.Beizhu)</td>
                        <td>@Html.GetDataValue_ID("userinfo", "全名", (int)item.MakeMan)</td>
                        <td><a href="/wms_chukumx/GetOutDetail/@item.ID" target="_blank"><i class="icon-list"></i></a></td>
                        <td><i class="icon-edit-sign"></i></td>
                        <td><a href="/wms_jianhuo/getpickdetailbyout?out=@item.ID" target="_blank"><i class="icon-hand-up"></i></a></td>
                        <td><a href="/quan_chukufh/outinventorycheck/@item.ID" target="_blank"><i class="icon-check"></i></a></td>
                        <td><i class="icon-barcode"></i></td>
                        <td><i class="icon-bookmark-empty"></i></td>
                        <td class="outOperateid"><i class="icon-print"><input type="hidden" class="input_outOperateid" value="@item.ID" /></i></td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
    <div class="pager">
        @Html.PagedListPager((IPagedList)Model, page => Url.Action("OutOperate", new { page }))
    </div>
</div>
<div id="pl_xz" class="panel panel-primary" style="display:none;position:fixed;top:50px;left:50px;width:1200px;height:600px;z-index:999;">
    <div class="panel-heading">
        <h3 class="panel-title">当前可用库存<button id="pl_xz_close" class="pull-right">X</button></h3>
    </div>
    <div class="panel-body">
        <div class="center">
            <div class="col-sm-1"><label>名称</label></div>
            <div class="col-sm-1"><input type="text" id="mc" size="12" /></div>
            <div class="col-sm-1"><label>批号</label></div>
            <div class="col-sm-1"><input type="text" id="ph" size="12" /></div>
            <div class="col-sm-1"><label>规格</label></div>
            <div class="col-sm-1"><input type="text" id="gg" size="12" /></div>
            <div class="col-sm-3"><p class="btn default" id="pl_search"><i class="icon-search"></i>查询</p></div>
            <div class="col-sm-3"><p class="btn default" id="pl_ok"><i class="icon-ok"></i>添加</p></div>

        </div>
        <div class="row" style="width:1190px;height:500px;overflow:auto">
            <table class="table table-condensed" id="dt_xz">
                <caption></caption>
                <thead>
                    <tr>
                        <td width="20px">出库</td>
                        <td>代码</td>
                        <td>商品</td>
                        <td>库存</td>
                        <td>锁定</td>
                        <td>单位</td>
                        <td>规格</td>
