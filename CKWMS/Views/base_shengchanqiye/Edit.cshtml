@using CKWMS.App_Code
@using CKWMS.Models
@model base_shengchanqiyeViewModel
@{
 ViewBag.Title="生产企业修改";
}

<div class="breadcrumbs" id="breadcrumbs">
<script type="text/javascript">
try { ace.settings.check('breadcrumbs', 'fixed') } catch (e) { }
</script>
<ul class="breadcrumb">
<li>
<i class="icon-home home-icon"></i>
<a href="/Home">首页</a>
</li>
<li>
<a href="/base_shengchanqiye">生产企业</a>
</li>
<li class="active"> 编辑</li>
</ul>
</div>

<div class="page-content">
@using(Html.BeginForm("Update","base_shengchanqiye",FormMethod.Post,new{ @role = "form" }))
{
@Html.AntiForgeryToken()
<input type="hidden" name="id" value="@ViewData.Model.ID"/>
<div class="page-header">
<p>
<button type="submit" class="btn btn-default"><i class="icon-save"></i>更新</button>
<button type="button" class="btn btn-default" onclick="javascript: history.back(-1); "><i class="icon-hand-left"></i>返回</button>
</p>
</div>
<div class="container">
<div class="form-group">
@Html.LabelFor(m=>m.Daima, new {@class="control-label col-sm-2"})
<div class="col-sm-10">
@Html.TextBoxFor(m => m.Daima, new { @class = "form-control" })
</div>
</div>
<div class="form-group">
@Html.LabelFor(m=>m.Qiyemingcheng, new {@class="control-label col-sm-2"})
<div class="col-sm-10">
@Html.TextBoxFor(m => m.Qiyemingcheng, new { @class = "form-control" })
</div>
</div>
<div class="form-group">
@Html.LabelFor(m=>m.YingyezhizhaoBH, new {@class="control-label col-sm-2"})
<div class="col-sm-10">
@Html.TextBoxFor(m => m.YingyezhizhaoBH, new { @class = "form-control" })
</div>
</div>
<div class="form-group">
@Html.LabelFor(m=>m.YingyezhizhaoYXQ, new {@class="control-label col-sm-2"})
<div class="col-sm-10">
<input type="date" class="form-control" name="YingyezhizhaoYXQ"  value="@string.Format("{0:yyyy-MM-dd}",ViewData.Model.YingyezhizhaoYXQ)">
</div>
</div>
<div class="form-group">
@Html.LabelFor(m=>m.YingyezhizhaoTP, new {@class="control-label col-sm-2"})
<div class="col-sm-10">
@Html.TextBoxFor(m => m.YingyezhizhaoTP, new { @class = "form-control" })
</div>
</div>
<div class="form-group">
@Html.LabelFor(m=>m.ShengchanxukeBH, new {@class="control-label col-sm-2"})
<div class="col-sm-10">
@Html.TextBoxFor(m => m.ShengchanxukeBH, new { @class = "form-control" })
</div>
</div>
<div class="form-group">
@Html.LabelFor(m=>m.ShengchanxukeYXQ, new {@class="control-label col-sm-2"})
<div class="col-sm-10">
<input type="date" class="form-control" name="ShengchanxukeYXQ"  value="@string.Format("{0:yyyy-MM-dd}",ViewData.Model.ShengchanxukeYXQ)">
</div>
</div>
<div class="form-group">
@Html.LabelFor(m=>m.ShengchanxukeTP, new {@class="control-label col-sm-2"})
<div class="col-sm-10">
@Html.TextBoxFor(m => m.ShengchanxukeTP, new { @class = "form-control" })
</div>
</div>
<div class="form-group">
@Html.LabelFor(m=>m.Shouying, new {@class="control-label col-sm-2"})
<div class="col-sm-10">
@Html.TextBoxFor(m => m.Shouying, new { @class = "form-control" })
</div>
</div>
<div class="form-group">
@Html.LabelFor(m=>m.MakeDate, new {@class="control-label col-sm-2"})
<div class="col-sm-10">
<input type="date" class="form-control" name="MakeDate"  value="@string.Format("{0:yyyy-MM-dd}",ViewData.Model.MakeDate)">
</div>
</div>
<div class="form-group">
@Html.LabelFor(m=>m.MakeMan, new {@class="control-label col-sm-2"})
<div class="col-sm-10">
@Html.SelectList_Auto("MakeMan", "userinfo","fullname",(long)ViewData.Model.MakeMan)
</div>
</div>
</div>
}
</div>

