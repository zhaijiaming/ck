@using CKWMS.App_Code
@using CKWMS.Models
@model base_shengchanqiyeViewModel
@{ViewBag.Title = "生产企业添加";}
@Html.GetFileUpload("yingyezhizhaotp", "yyzz", "z", "scqy", 3, 0, new Dictionary<string, string>() { { "yyzz", "yingyezhizhaotp" }, { "xktp", "shengchanxuketp" }, { "BATP", "beiantp" } })

<script type="text/javascript">
    try { ace.settings.check('breadcrumbs', 'fixed') } catch (e) { }

    $(document).ready(function () {
        $("#pl_Fanwei").draggable({ handle: '.tuodong' }, { containment: [-1000, 50, 1300, 600] });
        $("#YingyezhizhaoYXQ").datepicker();
        $("#ShengchanxukeYXQ").datepicker();
        $("#BeianYXQ").datepicker();
        $("#BeianPZRQ").datepicker();
        $("#XukePZRQ").datepicker();
        document.getElementById("daima").focus();

        var bj = 0;
        GetFanwei1();

        //显示全部
        $("#bt_fanwei_showall").click(function () {
            var bj = 1;
            var tempid = "";
            $("#qxdatatable tr input[type=checkbox]").each(function () {
                if (this.checked) {
                    tempid = tempid + $(this).parent().find(".xulieid").val() + ";";
                }
            });
            var fw = document.getElementsByClassName('xulieid');
            var i = $("#xukefanweidm").val();
            var ti = i.split(";");
            //ti.splice(ti.length - 1);
            var xiaolei = new Array();
            var x = 0;
            for (var k = 0; k < ti.length; k++, x = 0) {
                for (var l = 0; l < fw.length; l++) {
                    if (ti[k] == $(".xulieid").eq(l).val()) {
                        x = 1;
                    }
                }
                if (x != 1) {
                    xiaolei.push(ti[k]);
                }
            }
            for (k = 0; k < xiaolei.length; k++) {
                tempid = tempid + xiaolei[k] + ";";
            }
            tempid = tempid.substring(0, tempid.length - 1);
            GetFanwei(bj, tempid);
        });

        //显示大类
        $("#bt_fanwei_showdalei").click(function () {
            var bj = 1;
            var tempid = "";
            $("#qxdatatable tr input[type=checkbox]").each(function () {
                if (this.checked) {
                    tempid = tempid + $(this).parent().find(".xulieid").val() + ";";
                }
            });
            GetFanwei1(bj, tempid);
        });

        //关闭窗口
        $("#pl_close").click(function () {
            $("#pl_Fanwei").hide();
            var bj = 0;
            $("#guanlifenlei").val("全部");
            GetFanwei1(bj);
        });

        //添加范围
        $("#bt_fanwei_add").click(function () {
            var _jyfw = "";
            var _jyfw1 = "";
            var _jyfw2 = "";
            var _jyfw3 = "";
            var _jyfwjm = "";
            var t = document.getElementsByClassName('xulieid');
            $("#qxdatatable tr input[type=checkbox]").each(function () {
                if (this.checked) {
                    switch ($(this).parent().parent().parent().siblings().find(".glfl").html()) {
                        case "1": _jyfw1 = _jyfw1 + $(this).parent().parent().parent().siblings().find(".bh").html() + "," + $(this).parent().parent().parent().siblings().find(".mc").html() + ";";
                            break;
                        case "2": _jyfw2 = _jyfw2 + $(this).parent().parent().parent().siblings().find(".bh").html() + "," + $(this).parent().parent().parent().siblings().find(".mc").html() + ";";
                            break;
                        case "3": _jyfw3 = _jyfw3 + $(this).parent().parent().parent().siblings().find(".bh").html() + "," + $(this).parent().parent().parent().siblings().find(".mc").html() + ";";
                            break;
                    }
                    //_jyfw = _jyfw + $(this).parent().parent().parent().siblings().find(".glfl").html() + "," + $(this).parent().parent().parent().siblings().find(".bh").html() + "," + $(this).parent().parent().parent().siblings().find(".mc").html() + ";";
                    _jyfwjm = _jyfwjm + $(this).parent().find(".xulieid").val() + ";";
                }
            });
            _jyfw = "三类:" + _jyfw3 + "\n" + "二类:" + _jyfw2 + "\n" + "一类:" + _jyfw1;
            if (_jyfw3 == "")
                _jyfw = _jyfw.replace("三类:\n", "");
            if (_jyfw2 == "")
                _jyfw = _jyfw.replace("二类:\n", "");
            if (_jyfw1 == "")
                _jyfw = _jyfw.replace("一类:", "");

            var _addok = confirm("确定要修改经营范围吗？");
            if (_addok) {
                if (_jyfw.length >= 512) {
                    alert("所选数据超过范围，请重新选择。")
                }
                else {
                    $("#xukefanwei").val(_jyfw);
                    $("#xukefanweidm").val(_jyfwjm);
                    $("#pl_Fanwei").hide();
                    var bj = "0";
                }
                $("#guanlifenlei").val("全部");
                GetFanwei1(bj);
            }

        });
        //全选
        $("#bt_fanwei_all").click(function () {
            $(".xulieid").prop("checked", true);
        })
        //全部取消
        $("#bt_fanwei_clear").click(function () {
            $(".xulieid").prop("checked", false);
        })
        //选择管理分类
        $("#guanlifenlei").change(function () {
            var fl = document.getElementsByClassName('glfl');
            var lb = $("#guanlifenlei").find("option:selected").text();
            if (lb == "一类") {
                for (var i = 0; i < fl.length; i++) {
                    if ($(".glfl").eq(i).text() == "1") {
                        $(".bg").eq(i).show();
                    }
                    else { $(".bg").eq(i).hide(); }
                }
            }
            if (lb == "二类") {
                for (var i = 0; i < fl.length; i++) {
                    if ($(".glfl").eq(i).text() == "2") {
                        $(".bg").eq(i).show();
                    }
                    else { $(".bg").eq(i).hide(); }
                }
            }
            if (lb == "三类") {
                for (var i = 0; i < fl.length; i++) {
                    if ($(".glfl").eq(i).text() == "3") {
                        $(".bg").eq(i).show();
                    }
                    else { $(".bg").eq(i).hide(); }
                }
            }
            if (lb == "全部") {
                $(".bg").show();
            }
        });

        //BUTTON提交
        $("#save_top").click(function () {
            if ($("#daima").val() == "") {
                alert("代码不能为空");
                document.getElementById("shengcheng").focus();
            }
            else if ($("#shengcheng").val() == "") {
                alert("名称不能为空");
                document.getElementById("shengcheng").focus();
            }
            else { form.submit(); }
        })

        var form = $("#tempform");
        form.submit(function () {
            $.post(form.attr("action"),
                  form.serialize(),
                  function (result, status) {
                      //alert(status);
                      if (result._flag == 1) {
                          alert("保存成功");
                          location.href = "/base_shengchanqiye/Index";
                      }
                      else if (result._flag == -1) {
                          alert("保存失败");
                      }
                      else if (result._flag == -2) {
                          alert("保存失败，该代码已存在");
                      }
                      else { alert("未知异常"); }
                  },
                    "json");
            return false;

        });
        $("#save_bottom").click(function () {
            if ($("#daima").val() == "") {
                alert("代码不能为空");
                document.getElementById("shengcheng").focus();
            }
            else if ($("#shengcheng").val() == "") {
                alert("名称不能为空");
                document.getElementById("shengcheng").focus();
            }
            else { form.submit(); }
        })

        var form = $("#tempform");
        form.submit(function () {
            $.post(form.attr("action"),
                  form.serialize(),
                  function (result, status) {
                      //alert(status);
                      if (result._flag == 1) {
                          alert("保存成功");
                          location.href = "/base_shengchanqiye/Index";
                      }
                      else if (result._flag == -1) {
                          alert("保存失败");
                      }
                      else if (result._flag == -2) {
                          alert("保存失败，该代码已存在");
                      }
                      else { alert("未知异常"); }
                  },
                    "json");
            return false;

        });
        //照片的点击事件
        $(".yyzz_browse").click(function () {
            var img_url = $("#yyzz a").attr('href');
            if (img_url != null && img_url != "") {
                //console.log(img_url);
                window.open(img_url);
            }
        });
        $(".xktp_browse").click(function () {
            var img_url = $("#xktp a").attr('href');
            if (img_url != null && img_url != "") {
                //console.log(img_url);
                window.open(img_url);
            }
        });
        $(".BATP_browse").click(function () {
            var img_url = $("#BATP a").attr('href');
            if (img_url != null && img_url != "") {
                //console.log(img_url);
                window.open(img_url);
            }
        });
        //'是否合作'绑定值
        $("#HezuoSF").val("false");//默认赋值
        $("#HezuoSF").change(function () {
            if ($("#HezuoSF").is(":checked")) {
                $("#HezuoSF").val("true");//选中=true
            } else {
                $("#HezuoSF").val("false");//不选中=false
            }
        });
        //'是否审查'绑定值
        $("#ShenchaSF").val("false");//默认赋值
        $("#ShenchaSF").change(function () {
            if ($("#ShenchaSF").is(":checked")) {
                $("#ShenchaSF").val("true");//选中=true
            } else {
                $("#ShenchaSF").val("false");//不选中=false
            }
        });
    });
    //点击修改or增加
    function Bt_Fanweiadd() {
        document.getElementById("pl_Fanwei").removeAttribute("hidden");
        $("#pl_Fanwei").show();
    }
    function GetFanwei1(a, b) {
        var bj = a;
        var tempid = b;
        var filltable = "";
        $.post(
                "/base_qixiemulu/GetQixiemulu1",
                {

                },
                function (data) {
                    for (var i in data) {
                        filltable = filltable + '<tr class="bg"><td><div class="checkbox"><label><input type="checkbox" class ="xulieid" value="';
                        filltable = filltable + data[i].ID;
                        filltable = filltable + '"></label></div></td><td style="width:80px"><span class="bh">';
                        filltable = filltable + data[i].Bianhao;
                        filltable = filltable + '</span></td><td style="width:150px"><span class="mc">';
                        filltable = filltable + data[i].Mingcheng;
                        filltable = filltable + '</span></td><td style="width:800px">';
                        filltable = filltable + data[i].Miaoshu;
                        filltable = filltable + '</td><td style="width:100px"><span class="glfl">';
                        filltable = filltable + data[i].GuanliFL;
                        filltable = filltable + '</span></td></tr>';
                    }
                    $("#pl_Fanwei tbody").html(filltable);
                    GetData(bj, tempid)
                });
    }
    function GetFanwei(a, b) {
        var bj = a;
        var tempid = b;
        var filltable = "";
        $.post(
                "/base_qixiemulu/GetQixiemulu",
                {
                    //qxml: _qxmlid
                },
                function (data) {
                    for (var i in data) {
                        filltable = filltable + '<tr class="bg"><td><div class="checkbox"><label><input type="checkbox" class ="xulieid" value="';
                        filltable = filltable + data[i].ID;
                        filltable = filltable + '"></label></div></td><td style="width:80px"><span class="bh">';
                        filltable = filltable + data[i].Bianhao;
                        filltable = filltable + '</span></td><td style="width:150px"><span class="mc">';
                        filltable = filltable + data[i].Mingcheng;
                        filltable = filltable + '</span></td><td style="width:800px">';
                        filltable = filltable + data[i].Miaoshu;
                        filltable = filltable + '</td><td style="width:100px"><span class="glfl">';
                        filltable = filltable + data[i].GuanliFL;
                        filltable = filltable + '</span></td></tr>';
                    }
                    $("#pl_Fanwei tbody").html(filltable);
                    GetData(bj, tempid)
                });
    }
    //打勾
    function GetData(a, tempid) {
        var fw = document.getElementsByClassName('xulieid');
        var bj = a;
        var i = tempid;
        if (bj == 0) {
            var i = $("#xukefanweidm").val();
        }
        if (i != null) {
            var t = i.split(";");
        } else { t = "" }
        for (var k = 0; k < t.length; k++) {
            for (var l = 0; l < fw.length; l++) {
                if (t[k] == $(".xulieid").eq(l).val())
                    $("#qxdatatable tr input[type=checkbox]").eq(l).attr("checked", true);
            }
        }
    }

    function GetSpell() {
        $.post("/Service/GetSpellCode",
            {
                yourwords: $("#shengcheng").val()
            },
        function (response) {
            $("#daima").val(response);
        });
    }
</script>

<div class="breadcrumbs" id="breadcrumbs">
    <ul class="breadcrumb">
        <li>
            <i class="icon-home home-icon"></i>
            <a href="/Home">首页</a>
        </li>
        <li>
            <a href="/base_shengchanqiye">生产企业</a>
        </li>
        <li class="active"> 新增</li>
    </ul>
</div>

<div class="page-content">
    <div class="page-header">
        <button type="button" class="btn btn-app btn-xs btn-primary radius-4" onclick="javascript:history.back(-1); "><i class="icon-reply icon-only"></i><b>返回</b></button>
        <button type="button" id="save_top" class="btn btn-app btn-xs btn-primary radius-4">
            <i class="icon-save"></i><b>保存</b>
            <span class="badge badge-transparent">
                <i class="light-red icon-asterisk"></i>
            </span>
        </button>
    </div>
    <div class="wrapper" style="background: url('/Imgs/bg-cloud.png') repeat;">
        <div class="container">
            <div class="row">
                <div class="col-sm-8 col-sm-offset-2">
                    <div class="widget-box radius-4">
                        <div class="widget-header">
                            <h3><b>生产企业信息</b></h3>
                        </div>
                        <div class="widget-body">
                            <div class="widget-main no-padding">
                                @using (Html.BeginForm("Save", "base_shengchanqiye", FormMethod.Post, new { @role = "form", id = "tempform", @class = "form-horizontal" }))
                                {
                                    @Html.AntiForgeryToken()
                                    <input type="hidden" name="id" />
                                    <input type="hidden" id="btnSelect" value="" />
                                    <input type="hidden" id="MakeMan" name="MakeMan" value=@Session["user_id"] />
                                    <input type="hidden" id="Shouying" name="Shouying" value="1" />
                                    <fieldset>
                                        @*代码*@
                                        <div class="form-group">
                                            <label for="Daima" class="col-sm-3 control-label"><i class="red glyphicon glyphicon-asterisk"></i> <b>代码</b></label>
                                            <div class="col-sm-8">
                                                <input type="text" id="daima" name="Daima" class="form-control" placeholder="无需填写，由名称生成代码" />
                                            </div>
                                        </div>
                                        @*名称*@
                                        <div class="form-group">
                                            <label for="Qiyemingcheng" class="col-sm-3 control-label"><i class="red glyphicon glyphicon-asterisk"></i> <b>名称</b></label>
                                            <div class="col-sm-8">
                                                <div class="input-group">
                                                    <input id="shengcheng" name="Qiyemingcheng" placeholder="请填写名称" class="form-control" type="text">
                                                    <span class="input-group-btn">
                                                        <button type="button" onclick="GetSpell()" class="btn btn-primary btn-sm">
                                                            <b>生成代码</b>
                                                            <i class="icon-hand-up icon-on-right bigger-110"></i>
                                                        </button>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        @*营业执照编号*@
                                        <div class="form-group">
                                            <label for="YingyezhizhaoBH" class="col-sm-3 control-label"><b>营业执照编号</b></label>
                                            <div class="col-sm-8">
                                                <input type="text" name="YingyezhizhaoBH" class="form-control" placeholder="请填写营业执照编号" />
                                            </div>
                                        </div>
                                        @*营业执照有效期*@
                                        <div class="form-group">
                                            <label for="YingyezhizhaoYXQ" class="col-sm-3 control-label"><b>营业执照有效期</b></label>
                                            <div class="col-sm-8">
                                                <div class="input-group">
                                                    <input id="YingyezhizhaoYXQ" name="YingyezhizhaoYXQ" placeholder="请填写营业执照有效期" class="form-control" value="@DateTime.Now.ToString("yyyy-MM-dd")" type="date">
                                                    <span class="input-group-addon">
                                                        <i class="icon-calendar bigger-110"></i>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        @*营业执照照片*@
                                        <div class="form-group">
                                            <label for="YingyezhizhaoTP" class="col-sm-3 control-label"><b>营业执照照片</b></label>
                                            <div class="col-sm-8">
                                                <div class="input-group">
                                                    <input id="yingyezhizhaotp" name="yingyezhizhaotp" readonly placeholder="请上传照片" class="form-control" type="text">
                                                    <span class="input-group-btn">
                                                        <button type="button" data-toggle="modal" data-target="#myModalUpload" onclick="btn1()" class="btn btn-primary btn-sm">
                                                            <b>上传</b>
                                                            <i class="icon-cloud-upload icon-on-right bigger-110"></i>
                                                        </button>
                                                        <span hidden id="yyzz"></span>
                                                        <button type="button" class="btn btn-primary btn-sm yyzz_browse">
                                                            <b>浏览</b>
                                                            <i class="icon-camera icon-on-right bigger-110"></i>
                                                        </button>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        @*生产许可编号*@
                                        <div class="form-group">
                                            <label for="ShengchanxukeBH" class="col-sm-3 control-label"><b>生产许可编号</b></label>
                                            <div class="col-sm-8">
                                                <input type="text" name="ShengchanxukeBH" class="form-control" placeholder="请填写生产许可编号" />
                                            </div>
                                        </div>
                                        @*生产许可有效期*@
                                        <div class="form-group">
                                            <label for="ShengchanxukeYXQ" class="col-sm-3 control-label"><b>生产许可有效期</b></label>
                                            <div class="col-sm-8">
                                                <div class="input-group">
                                                    <input id="ShengchanxukeYXQ" name="ShengchanxukeYXQ" placeholder="生产许可有效期" class="form-control" value="@DateTime.Now.ToString("yyyy-MM-dd")" type="date">
                                                    <span class="input-group-addon">
                                                        <i class="icon-calendar bigger-110"></i>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        @*生产许可照片*@
                                        <div class="form-group">
                                            <label for="ShengchanxukeTP" class="col-sm-3 control-label"><b>生产许可照片</b></label>
                                            <div class="col-sm-8">
                                                <div class="input-group">
                                                    <input id="shengchanxuketp" name="shengchanxuketp" readonly placeholder="请上传照片" class="form-control" type="text">
                                                    <span class="input-group-btn">
                                                        <button type="button" data-toggle="modal" data-target="#myModalUpload" onclick="btn2()" class="btn btn-primary btn-sm">
                                                            <b>上传</b>
                                                            <i class="icon-cloud-upload icon-on-right bigger-110"></i>
                                                        </button>
                                                        <span hidden id="xktp"></span>
                                                        <button type="button" class="btn btn-primary btn-sm xktp_browse">
                                                            <b>浏览</b>
                                                            <i class="icon-camera icon-on-right bigger-110"></i>
                                                        </button>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        @*生产许可批准日期*@
                                        <div class="form-group">
                                            <label for="XukePZRQ" class="col-sm-3 control-label"><b>生产许可批准日期</b></label>
                                            <div class="col-sm-8">
                                                <div class="input-group">
                                                    <input id="XukePZRQ" name="XukePZRQ" placeholder="生产许可批准日期" class="form-control" value="@DateTime.Now.ToString("yyyy-MM-dd")" type="date">
                                                    <span class="input-group-addon">
                                                        <i class="icon-calendar bigger-110"></i>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        @*生产许可发证机关*@
                                        <div class="form-group">
                                            <label for="XukeFZJG" class="col-sm-3 control-label"><b>生产许可发证机关</b></label>
                                            <div class="col-sm-8">
                                                <input type="text" name="XukeFZJG" class="form-control" placeholder="请填写生产许可发证机关" />
                                            </div>
                                        </div>
                                        @*生产许可范围*@
                                        <div class="form-group">
                                            <label for="Xukefanwei" class="col-sm-3 control-label"><b>生产许可范围</b></label>
                                            <div class="col-sm-8">
                                                <button type="button" class="btn btn-primary btn-sm" id="bt_Fanweiadd" onclick="Bt_Fanweiadd()"><b>增加</b><i class="icon-hand-up icon-on-right bigger-110"></i></button>
                                                <textarea type="text" style="resize:vertical;" rows="6" class="form-control" readonly id="xukefanwei" name="Xukefanwei"></textarea>
                                            </div>
                                        </div>
                                        @*生产许可范围简码*@
                                        <div class="form-group" style="display:none">
                                            <label for="XukefanweiDM" class="col-sm-3 control-label"><b>生产许可范围简码</b></label>
                                            <div class="col-sm-8">
                                                <input type="text" class="form-control " id="XukefanweiDM" name="XukefanweiDM">
                                            </div>
                                        </div>
                                        @*备案编号*@
                                        <div class="form-group">
                                            <label for="BeianBH" class="col-sm-3 control-label"><b>备案编号</b></label>
                                            <div class="col-sm-8">
                                                <input type="text" name="BeianBH" class="form-control" placeholder="请填写备案编号" />
                                            </div>
                                        </div>
                                        @*备案有效期*@
                                        <div class="form-group">
                                            <label for="BeianYXQ" class="col-sm-3 control-label"><b>备案有效期</b></label>
                                            <div class="col-sm-8">
                                                <div class="input-group">
                                                    <input id="BeianYXQ" name="BeianYXQ" placeholder="备案有效期" class="form-control" value="@DateTime.Now.ToString("yyyy-MM-dd")" type="date" style="z-index:1000;position:relative;">
                                                    <span class="input-group-addon">
                                                        <i class="icon-calendar bigger-110"></i>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        @*备案照片*@
                                        <div class="form-group">
