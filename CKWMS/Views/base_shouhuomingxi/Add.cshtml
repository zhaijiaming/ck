@using CKWMS.App_Code
@using CKWMS.Models
@model base_shouhuomingxiViewModel
@{ViewBag.Title="收货明细添加";}

<div class="breadcrumbs" id="breadcrumbs">
<script type="text/javascript">
try { ace.settings.check('breadcrumbs', 'fixed') } catch (e) { }
</script>
<ul class="breadcrumb">
<li>
<i class="icon-home home-icon"></i>
<a href="/Home">首页</a>
</li>
<li>
<a href="/base_shouhuomingxi">收货明细</a>
</li>
<li class="active"> 新增</li>
</ul>
</div>

<div class="page-content">
@using (Html.BeginForm("Save","base_shouhuomingxi",FormMethod.Post,new{@role ="form" }))
{
@Html.AntiForgeryToken()
<input type="hidden" name="id"/>
<div class="page-header">
<p>
<button type="submit" class="btn btn-default"><i class="icon-save"></i>保存</button>
<button type="button" class="btn btn-default" onclick="javascript:history.back(-1); "><i class="icon-hand-left"></i>返回</button>
</p>
</div>
<div class="container">
<div class="form-group">
@Html.LabelFor(m=>m.ShouhuofangID,new{ @class ="control-label col-sm-2"})
<div class="col-sm-10">
@Html.TextBoxFor(m=>m.ShouhuofangID,new{@class="form-control"})
</div>
</div>
<div class="form-group">
@Html.LabelFor(m=>m.Mingcheng,new{ @class ="control-label col-sm-2"})
<div class="col-sm-10">
@Html.TextBoxFor(m=>m.Mingcheng,new{@class="form-control"})
</div>
</div>
<div class="form-group">
@Html.LabelFor(m=>m.Dizhi,new{ @class ="control-label col-sm-2"})
<div class="col-sm-10">
@Html.TextBoxFor(m=>m.Dizhi,new{@class="form-control"})
</div>
</div>
<div class="form-group">
@Html.LabelFor(m=>m.Lianxiren,new{ @class ="control-label col-sm-2"})
<div class="col-sm-10">
@Html.TextBoxFor(m=>m.Lianxiren,new{@class="form-control"})
</div>
</div>
<div class="form-group">
@Html.LabelFor(m=>m.Lianxidianhua,new{ @class ="control-label col-sm-2"})
<div class="col-sm-10">
@Html.TextBoxFor(m=>m.Lianxidianhua,new{@class="form-control"})
</div>
</div>
<div class="form-group">
@Html.LabelFor(m=>m.XiaoshouID,new{ @class ="control-label col-sm-2"})
<div class="col-sm-10">
@Html.TextBoxFor(m=>m.XiaoshouID,new{@class="form-control"})
</div>
</div>
<div class="form-group">
@Html.LabelFor(m=>m.Xiaoshouren,new{ @class ="control-label col-sm-2"})
<div class="col-sm-10">
@Html.TextBoxFor(m=>m.Xiaoshouren,new{@class="form-control"})
</div>
</div>
<div class="form-group">
@Html.LabelFor(m=>m.MakeDate,new{ @class ="control-label col-sm-2"})
<div class="col-sm-10">
<input type="date" class="form-control" value="@DateTime.Now.ToString("yyyy-MM-dd")" name="MakeDate" >
</div>
</div>
<div class="form-group">
@Html.LabelFor(m=>m.MakeMan,new{ @class ="control-label col-sm-2"})
<div class="col-sm-10">
@Html.SelectList_Auto("MakeMan", "userinfo","fullname")
</div>
</div>
</div>
}
</div>

